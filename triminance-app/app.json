{
  "expo": {
    "name": "triminance",
    "slug": "triminance",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "packagerOpts": {
      "config": "metro.config.js"
    },
    "splash": {
      "image": "./assets/img/logotriminance.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.app.triminance"
    },
    "android": {
      "package": "com.app.triminance",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/logotriminance.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
